{
    "Version": "2008-10-17",
    "Statement": [
        {
            "Sid": "read-only",
            "Effect": "Allow",
            "Principal": {
                "AWS": "*"
            },
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::{{ bucket_name }}/*"
        }
    ]
}